// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Consultation {
  id            Int      @id @default(autoincrement())
  patientName   String
  doctorName    String
  selectedDate  String
  duration      String
  description   String?
}
